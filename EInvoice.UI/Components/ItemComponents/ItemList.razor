<tr>
    <td>@ItemCode</td>
    <td>@ItemName</td>
    <td>
        <a class="btn btn-outline-warning">
            <i class="bi bi-pencil-square"></i>
        </a>
        <a class="btn btn-outline-danger">
            <i class="bi bi-trash"></i>
        </a>
    </td>
</tr>

@code {
    [Parameter]
    public string ItemName { get; set; }
    [Parameter]
    public string ItemCode { get; set; }
}